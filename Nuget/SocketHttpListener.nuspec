        <dependencies>
            <dependency id="Patterns.Logging" version="1.0.0.6" />
        </dependencies>
    </metadata>
    <files>
        <file src="dlls\SocketHttpListener.dll" target="lib\net45\SocketHttpListener.dll" />
    </files>
</package>
